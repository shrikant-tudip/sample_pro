<!DOCTYPE html>
<html>
<head>
  <title>SamplePro</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= stylesheet_link_tag    'http://netdna.bootstrapcdn.com/bootstrap/3.0.3/css/bootstrap.min.css' %>
  <%= stylesheet_link_tag    'http://netdna.bootstrapcdn.com/font-awesome/4.0.3/css/font-awesome.min.css' %>
  <%= stylesheet_link_tag    'http://fonts.googleapis.com/css?family=Lora:400,700,400italic,700italic' %>
  <%= stylesheet_link_tag    'http://fonts.googleapis.com/css?family=Montserrat:400,700' %>
  <%= stylesheet_link_tag    'grayscale.css' %>
  
  <%= javascript_include_tag 'application'%>
  <%= javascript_include_tag 'https://ajax.googleapis.com/ajax/libs/jquery/1.10.2/jquery.min.js' %>
  <%= javascript_include_tag 'http://netdna.bootstrapcdn.com/bootstrap/3.0.3/js/bootstrap.min.js' %>
  <%= javascript_include_tag 'http://cdnjs.cloudflare.com/ajax/libs/jquery-easing/1.3/jquery.easing.min.js' %>
  <%= javascript_include_tag 'grayscale.js' %>
  <%= csrf_meta_tags %>
</head>
<body id="page-top" data-spy="scroll" data-target=".navbar-custom">

<%= yield %>

</body>
</html>